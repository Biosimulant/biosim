{
  "name": "@biosim/simui-ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./src/styles/globals.css"
  },
  "files": [
    "dist",
    "src/styles/globals.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode library && npm run build:types",
    "build:static": "vite build --mode static",
    "build:types": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist dist-static"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "dependencies": {
    "@xyflow/react": "^12.3.6",
    "dagre": "^0.8.5",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "jsdom": "^28.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vitest": "^2.1.1"
  }
}
